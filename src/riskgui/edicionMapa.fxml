<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="vBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="riskgui.RepartoPaisesController">
   <children>
      <ToolBar fx:id="toolbar" prefHeight="40.0" prefWidth="200.0">
         <items>
            <Button fx:id="bSiguiente" mnemonicParsing="false" onAction="#siguiente" text="Siguiente" />
         </items>
      </ToolBar>
      <Pane fx:id="panelMapa" VBox.vgrow="ALWAYS">
         <children>
            <Group fx:id="grupoPaises" />
         </children>
         <VBox.margin>
            <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
         </VBox.margin>
      </Pane>
   </children>
</VBox>
