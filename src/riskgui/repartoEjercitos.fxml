<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="vBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="riskgui.RepartoEjercitosController"  stylesheets="@style.css">
   <children>
      <ToolBar fx:id="toolbar" prefHeight="40.0" prefWidth="200.0">
         <items>
            <ImageView fx:id="imgSoldado" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@resources/soldado.png" />
               </image>
            </ImageView>
         </items>
      </ToolBar>
      <Pane fx:id="panelMapa" VBox.vgrow="ALWAYS">
         <children>
            <Group fx:id="grupoPaises" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
         </VBox.margin>
      </Pane>
   </children>
</VBox>
